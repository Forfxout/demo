{
  "name": "tex",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@mapbox/leaflet-omnivore": "^0.3.4",
    "@reduxjs/toolkit": "^1.2.3",
    "autobahn": "^19.10.1",
    "babel-runtime": "^6.26.0",
    "build-url": "^1.3.3",
    "core-js": "^2.6.10",
    "crypto-js": "^3.1.9-1",
    "d3-scale-chromatic": "^1.5.0",
    "dplayer": "^1.25.0",
    "es-abstract": "^1.16.0",
    "he": "^1.2.0",
    "leaflet": "^1.6.0",
    "leaflet-dvf": "^0.3.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "redux": "^4.0.4",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.3",
    "tinycolor2": "^1.4.1",
    "vue": "^2.6.10",
    "vue-flag-icon": "^1.0.6",
    "vue-fragment": "^1.5.1",
    "vue-moment": "^4.1.0",
    "vue-plotly": "^1.1.0",
    "vue-router": "^3.1.3",
    "vue-virtual-scroll-list": "^1.4.2",
    "vue2-leaflet": "^2.4.2",
    "vuetify": "^2.2.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/runtime": "^7.7.2",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "rxjs-marbles": "^5.0.4",
    "sass": "^1.23.3",
    "sass-loader": "^7.3.1",
    "vue-cli-plugin-vuetify": "^1.1.2",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.4.3"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!vuetify)"
    ],
    "setupFiles": [
      "<rootDir>/tests/unit/setup.js"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "indent": [
        "error",
        4
      ],
      "vue/html-indent": [
        "error",
        4,
        {
          "attribute": 1,
          "baseIndent": 0,
          "closeBracket": 0,
          "alignAttributesVertically": true,
          "ignores": []
        }
      ],
      "vue/require-v-for-key": [
        "none"
      ],
      "lines-between-class-members": [
        "error",
        "always",
        {
          "exceptAfterSingleLine": true
        }
      ],
      "no-underscore-dangle": [
        2,
        {
          "allowAfterThis": true
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": true,
          "ignorePropertyModificationsFor": [
            "state"
          ]
        }
      ],
      "no-use-before-define": [
        "error",
        {
          "functions": false
        }
      ],
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "import/prefer-default-export": [
        "none"
      ],
      "import/no-cycle": [
        "none"
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
